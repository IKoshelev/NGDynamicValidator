	"typescript" : "*",
	"grunt-ts": "*",
    "bower": "~1.3.3",
    "grunt-bower-install": "~1.4.1",
    "karma": "~0.12.14",
    "karma-cli": "0.0.4",
    "karma-chrome-launcher": "~0.1.3",
    "karma-ie-launcher": "~0.1.5",
    "karma-firefox-launcher": "~0.1.3",
    "karma-html-reporter": "~0.2.3",
